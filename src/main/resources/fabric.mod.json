{
  "schemaVersion": 1,
  "id": "apostlehack",
  "version": "${version}",
  "name": "ApostleHack",
  "description": "An addon template for Meteor Client addons.",
  "authors": [
    "Apostle's"
  ],
  "icon": "assets/template/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "com.apostles.apostlehack.ApostleHack"
    ]
  },
  "mixins": [
    "addon-template.mixins.json"
  ],
  "custom": {
    "meteor-client:color": "0,0,0"
  },
  "depends": {
    "java": ">=21",
    "minecraft": ["${mc_version}"],
    "meteor-client": "*"
  }
}
